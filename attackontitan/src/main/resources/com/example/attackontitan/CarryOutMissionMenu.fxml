<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="650.0" prefWidth="950.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.attackontitan.Controller.CarryOutMissionMenuController">
   <children>
      <ImageView fx:id="backgroundView" fitHeight="670.0" fitWidth="1007.0" layoutX="-5.0" layoutY="-6.0" />
      <Text fx:id="startingPoint" fill="WHITE" layoutX="79.0" layoutY="108.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter starting point: ">
         <font>
            <Font name="Gill Sans MT" size="35.0" />
         </font></Text>
      <Button fx:id="backToPreviousButton" layoutX="846.0" layoutY="31.0" mnemonicParsing="false" onAction="#backToPrevious" prefHeight="50.0" prefWidth="80.0" style="-fx-background-color: GOLD;" text="Back">
         <font>
            <Font name="Gill Sans MT" size="20.0" />
         </font></Button>
      <Button fx:id="checkButton" layoutX="559.0" layoutY="72.0" mnemonicParsing="false" onAction="#check" prefHeight="46.0" prefWidth="80.0" style="-fx-background-color: gold;" text="Check">
         <font>
            <Font name="Gill Sans MT" size="20.0" />
         </font></Button>
      <TextField fx:id="startingPointTextField" layoutX="372.0" layoutY="73.0" opacity="0.78" prefHeight="46.0" prefWidth="150.0" />
      <Label fx:id="errorLabel" layoutX="372.0" layoutY="125.0" prefHeight="18.0" prefWidth="420.0" text="Label" textFill="RED" />
      <Label fx:id="resultLabel" layoutX="59.0" layoutY="184.0" prefHeight="230.0" prefWidth="879.0" text="Label" textFill="WHITE">
         <font>
            <Font name="Gill Sans MT Bold" size="16.0" />
         </font></Label>
   </children>
</AnchorPane>
